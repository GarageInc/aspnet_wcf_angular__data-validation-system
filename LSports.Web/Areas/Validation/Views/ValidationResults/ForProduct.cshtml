@section Body
{

    <style>
        .dataTables_filter {
            text-align: right;
        }

        .table-toolbar {
            margin-bottom: 0px;
            padding-left: 10px;
            padding-right: 10px;
        }

        .bordered {
            border: 3px solid;
            cursor: pointer;
        }

        .pointer {
            cursor: pointer;
        }

        .bordered-image:hover {
            border: 1px solid;
            cursor: pointer;
        }

        .circle {
	        width: 20px;
	        height: 20px;
	        -moz-border-radius: 10px;
	        -webkit-border-radius: 0px;
	        border-radius: 10px;
        }

        .inline {
            display: inline-block;
        }

        .custom_badge {
            position: absolute;
            top: 0px;
            left: 40px;
            margin-left: 10px;
        }

        .low_priority_arrow {
            width: 25px;
            content:url('data:image/png;base64,');
        }
        .high_priority_arrows {
            width: 25px;
            content:url('data:image/png;base64,');
        }
        .medium_priority_arrows {
            width: 25px;
            content:url('data:image/png;base64,');
        }

        .relative {
            position: relative;
        }
        
        .absolute {
            position: absolute;
        }

        .l30p {
            left: 30px;
        }

        .editable-clear-x {
            right: 6px !important;
        }
    </style>


