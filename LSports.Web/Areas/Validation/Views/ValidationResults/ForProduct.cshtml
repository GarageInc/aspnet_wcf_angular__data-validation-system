                                        ng-model="filterObject.Event">
                                    <option value="" selected>Event..</option>
                                </select>
                            </div>
                            <div class="inline">
                                <select select2="" ng-options="filter as filter.Name for filter in markets" style="display: inline-block;" ng-disabled="isLoading"
