                                        ng-model="filterObject.Sport">
                                    <option value="" selected>Sport..</option>
                                </select>
                            </div>
                            <div class="inline">
                                <select select2="" ng-options="filter as filter.Name for filter in countries" style="display: inline-block;" ng-disabled="isLoading"
