                                        ng-model="filterObject.League">
                                    <option value="" selected>League..</option>
                                </select>
                            </div>
                            <div class="inline">
                                <select select2="" ng-options="filter as filter.Name for filter in events" style="display: inline-block;" ng-disabled="isLoading"
