                    <a href="/Product">Products</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <a href="/ValidationResults/ForProduct/{{productId}}">Alerts</a>
                </li>
            </ul>
        </div>

            @Html.Hidden("productId", (string)ViewBag.ProductId)

            <div ng-init="refresh()">

                <div class="col-md-12">

                    <div class="portlet box">
                        <div class="block-ui" ng-if="isLoading">
                            <div class="loading-message"><img src="http://cdn.lsports.eu//Content/Metronic/theme/assets/global/img/loading-spinner-grey.gif" ><span>&nbsp;&nbsp;LOADING...</span></div>
                        </div>

                        <div class="row col-md-12" style="">
