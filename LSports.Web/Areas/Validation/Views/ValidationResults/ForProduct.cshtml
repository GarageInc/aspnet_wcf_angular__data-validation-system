                                        ng-model="filterObject.Country">
                                    <option value="" selected>Country..</option>
                                </select>
                            </div>
                            <div class="inline">
                                <select select2="" ng-options="filter as filter.Name for filter in leagues" style="display: inline-block;" ng-disabled="isLoading"
