                            <button type="button" class="close" style="margin: 10px;" data-dismiss="modal" aria-hidden="true"></button>
                            <div class="page-bar" style="margin-bottom: 0px;">
                                <ul class="page-breadcrumb">
                                    <li>
                                        <a href="#">Setting #{{selectedValidationSetting.Setting.Id}}</a>
                                        <i class="fa fa-angle-right"></i>
                                    </li>
                                    <li>
                                        <a href="#">Grouped By Events</a>
                                        <i class="fa fa-angle-right"></i>
                                    </li>
                                    <li>
                                        <a href="#">EventId #{{selectedEventId}}</a>
                                        <i class="fa fa-angle-right"></i>
                                    </li>
                                    <li>
                                        <a href="#">Validation #{{selectedValidationResult.Id}}</a>
                                    </li>
                                </ul>
                            </div>
                            <div class="modal-header" style="position: relative;">
                                <div ng-show="!selectedValidationResult.IsActive" title="This event was disabled by DVS or moved in historical by User" 
                                     style="position: absolute;right: 20px;padding: 5px 10px 5px 10px;border: 1px solid;border-radius: 5px !important;color: white;background-color: mediumaquamarine;">Historical event(not active)</div>
                                <div ng-show="selectedValidationResult.IsActive" title="This event was created/activated by DVS"
                                     style="position: absolute;right: 20px;padding: 5px 10px 5px 10px;border: 1px solid;border-radius: 5px !important;color: white;background-color: tomato;">Active event</div>
                                <h4><b>#{{selectedValidationResult.Id}} (EventId: {{selectedValidationResult.EventId}})</b></h4>
                                <h5>{{selectedProduct.Name}} - {{selectedValidationSetting.Setting.Name}}</h5>
                                <h5><a>{{selectedValidationResult.SportName}}</a> <img style="width: 10px;" src="https://image.freepik.com/free-icon/no-translate-detected_318-33109.jpg"/> <a>{{selectedValidationResult.LocationName}}</a> <img style="width: 10px;" src="https://image.freepik.com/free-icon/no-translate-detected_318-33109.jpg"/> <a>{{selectedValidationResult.LeagueName}}</a> <img style="width: 10px;" src="https://image.freepik.com/free-icon/no-translate-detected_318-33109.jpg"/> {{selectedValidationResult.EventName}}</h5>
                               
                                <h5 ng-show="canReduceMarket()">Markets: <b>{{selectedValidationResult.Market}}</b></h5>
                                <h5 ng-show="canReduceProvider()">Providers: <b>{{selectedValidationResult.Provider}}</b></h5>
                                
                                <div style="border: 1px solid; padding: 10px; min-height: 40px;">
                                    <settingexpression-directive id="'settingExression_2'" expression="selectedValidationSetting.Setting.Expression" rules="validationRules">
                                    </settingexpression-directive>
                                </div>
                                <div style="overflow: auto; border: 1px solid; margin-top: 5px;" ng-if="isShowingRules">
                                    <table class="table table-striped table-bordered">
                                        <thead>
                                        <tr role="row">
                                            <th rowspan="1" colspan="1" style="width: 10%;">Rule Name</th>
                                            <th rowspan="1" colspan="1" style="width: 20%;">Property Name</th>
                                            <th rowspan="1" colspan="1" style="width: 20%;">For all nodes</th>
                                            <th rowspan="1" colspan="1" style="width: 20%;">Parameter</th>
                                            <th rowspan="1" colspan="1" style="width: 20%;">Operator</th>
                                            <th rowspan="1" colspan="1" style="width: 20%;">Value</th>
                                            <th style="width: 10%;"></th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <tr role="row" ng-repeat="rule in validationRules">
                                            <td style="width: 10%;">
                                                <a href="#" class="{{'suffix' + 'RuleName' + rule.Id}}">
                                                    {{rule.RuleName}}
                                                </a>
                                            </td>
                                            <td style="width: 20%;">
                                                <a href="#" class="{{'suffix' + 'PropertyName' + rule.Id}}" data-type="select" data-value="">
                                                </a>
                                            </td>
                                            <td style="width: 5%;">
                                                <input type="checkbox" ng-model="rule.IsForAllNodes" disabled/>
                                            </td>
                                            <td style="width: 10%;">
                                                <a href="#" class="{{'suffix' + 'ParameterName' + rule.Id}}" data-type="select" data-value="">
                                                </a>
                                            </td>
                                            <td style="width: 10%;">
                                                <a href="#" class="{{'suffix' + 'OperatorName' + rule.Id}}" data-type="select" data-value="">
                                                </a>
                                            </td>
                                            <td style="width: 20%;">
                                                <a href="#" class="{{'suffix' + 'ValueName' + rule.Id}}" data-pk="1" data-value="" data-name="Name">
                                                </a>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div style="margin-top: 10px;">
                                    <div style="margin-right: 20px; display: inline-block;" ng-show="canReduceMarket()">
                                        <h5 class="inline">For market <b>({{selectedValidationResult.Market}})</b>:</h5>
                                        <input class="inline" type="checkbox" ng-model="isForMarket" ng-change="loadBySavedXml()"/>
                                    </div>
                                    <div style="margin-right: 20px; display: inline-block;" ng-show="canReduceProvider()">
                                        <h5 class="inline">For provider<b>({{selectedValidationResult.Provider}})</b>:</h5>
                                        <input class="inline" type="checkbox" ng-model="isForProvider" ng-change="loadBySavedXml()" />
                                    </div>
                                    <div style="display: inline-block; margin-right: 20px;">
                                        <h5 class="inline">Show historical attributes:</h5>
                                        <input class="inline" type="checkbox" ng-model="isHistoricalAttributes" ng-change="changeIsHistoricalAttributes()"/>
                                    </div>
                                    <div style="display: inline-block; margin-right: 20px;">
                                        <h5 class="inline">Show Header:</h5>
                                        <input class="inline" type="checkbox" ng-model="isWithHeader" ng-change="loadBySavedXml()"/>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-body">
                                <div style="overflow: auto; border: 1px solid; margin: 10px; position: relative; margin: 0 auto; padding: 10px;" id="xmlTree">
                                    <pre lang="xml" id="xmlValue">                                        
                                    </pre>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
            @Html.Partial("../ValidationSettings/Partial")

        </div>
    </div>

