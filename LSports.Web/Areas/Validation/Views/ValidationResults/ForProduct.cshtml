                                <input id="checkIntervalOfReload" type="checkbox" ng-model="updateByInterval" ng-change="checkboxIntervalChanged()" />
                            </div>
                        </div>

                        <div class="row col-md-12" style="padding: 0px; margin-top: 10px;">
